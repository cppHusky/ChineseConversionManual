{
    "latex-workshop.intellisense.command.user": {
        "bopin{}{}":"bopin{${1:zhuyin}}{${2:pinyin}}",
        "boxpinyin{}":"boxpinyin{${1:pinyin}}",
        "boxzhuyin{}":"boxzhuyin{${1:zhuyin}}",
        "charcode{}":"charcode{${1:code}}",
        "content{}{}":"content{${1}}{${2}}",
        "includecharacter[]{}":"includecharacter[${1:options}]{${2:name}}",
        "includecharacter{}":"includecharacter{${1:name}}",
        "inlineinfo{}{}":"inlineinfo{${1:type}}{${2:text}}",
        "inlinelinkinfo{}{}":"inlinelinkinfo{${1:link}}{${2:text}}",
        "subsubsubsection{}":"subsubsubsection{${1}}",
        "shiftbaseline[]{}":"shiftbaseline[${1:height}]{${2:text}}",
        "shiftbaseline{}":"shiftbaseline{${1:text}}",
    },
    "latex-workshop.intellisense.package.extra": [
        "booktabs",
        "cleveref",
        "fancyhdr",
        "fontspec",
        "hyperref",
        "nicematrix",
        "pxrubrica",
        "setspace",
        "subcaption", 
        "tcolorbox",
        "tikz",
        "titlesec",
        "wrapfig",
        "xfakebold",
        "xparse",
        "xpinyin",
    ],
    "latex-workshop.latex.recipe.default": "first",
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.outDir": "%WORKSPACE_FOLDER%/output",
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk (xelatex)",
            "tools": [
                "xelatexmkT",
                "xelatexmkS"
            ]
        },
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "xelatexmkT",
            "command": "latexmk",
            "args": [
                "-synctex=0",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-auxdir=%OUTDIR%/tmp",
                "-outdir=%OUTDIR%/release",
                "%WORKSPACE_FOLDER%/src/manualT.tex"
            ],
            "env": {}
        },
        {
            "name": "xelatexmkS",
            "command": "latexmk",
            "args": [
                "-synctex=0",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-auxdir=%OUTDIR%/tmp",
                "-outdir=%OUTDIR%/release",
                "%WORKSPACE_FOLDER%/src/manualS.tex"
            ],
            "env": {}
        }
    ],
    "latex-workshop.view.pdf.trim": 1,
    "latex-workshop.view.pdf.zoom": "page-width",
    "vim.vimrc.enable": true,
}